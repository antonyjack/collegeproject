@model  IEnumerable<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel>
@{
    ViewBag.Title = "Home Page";

    string BasePath = "/images/";

    int Total = Model.Count();
    decimal Page = 0;
    List<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel> One = new List<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel>();
    List<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel> Two = new List<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel>();
    List<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel> Three = new List<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel>();
    List<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel> Four = new List<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel>();
    List<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel> Five = new List<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel>();
    List<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel> Six = new List<DynamicPhotoGallary.Models.ViewModel.MainImageViewModel>();
    if (Total > 0)
    {
        Page = Total / 6;
        Page = Math.Round(Page);
        if (Page > 0)
        {
            for (int i = 0; i < Page; i++)
            {
                var TempList = Model.Skip(i).Take(6).ToArray();
                if (TempList.Length > 0)
                {
                    One.Add(TempList[0]);
                }

                if (TempList.Length > 1)
                {
                    Two.Add(TempList[1]);
                }

                if (TempList.Length > 2)
                {
                    Three.Add(TempList[2]);
                }

                if (TempList.Length > 3)
                {
                    Four.Add(TempList[3]);
                }

                if (TempList.Length > 4)
                {
                    Five.Add(TempList[4]);
                }

                if (TempList.Length > 5)
                {
                    Six.Add(TempList[5]);
                }
            }
        }
        else
        {
            var TempList = Model.ToArray();
            if (TempList.Length > 0)
            {
                One.Add(TempList[0]);
            }

            if (TempList.Length > 1)
            {
                Two.Add(TempList[1]);
            }

            if (TempList.Length > 2)
            {
                Three.Add(TempList[2]);
            }

            if (TempList.Length > 3)
            {
                Four.Add(TempList[3]);
            }

            if (TempList.Length > 4)
            {
                Five.Add(TempList[4]);
            }

            if (TempList.Length > 5)
            {
                Six.Add(TempList[5]);
            }
        }

    }

}
